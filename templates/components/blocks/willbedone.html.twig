<div class="section-header">
    <div class="background" id="prosadime"></div>
    <div class="container text-center">
        <h2>Prosadíme</h2>
        <p>V dalších letech bychom rádi zařídili:</p>
    </div>
</div>
<div class="gallery">
    {% for element in randomWillBeDone  %}
        <a href="{{ path(element.path) }}" class="block link">
            <img src="{{ asset(element.photo) }}" alt="">
            <div class="move-top">
                {% if element.text[0] != '' %}
                    <div class="btn secondary">{{ element.text[0]|raw }}</div> <br>
                {% endif %}
            </div>
            <div class="bottom-text">{{ element.text[1]|raw }}</div>
        </a>
    {% endfor %}
</div>
<br>
<div class="text-center">
    <a href="{{ path('willBeDone') }}" class="btn x-large secondary">Zobrazit více</a>
</div>
<br><br>
